<script>
    // Quick note
    document.getElementById('quick-note-link').addEventListener('click', function (item) {
        modal = document.getElementById("large-modal-content");

        loadUrl("/note/new").then(data => {
            loadUrl("/note/" + data + "/edit").then(data => {
                setInnerHtml(modal, data);
            });
        });
    });
</script>